{
  "name": "testupdate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5"
  },
  "build": {
    "appId": "com.yourdomain.testupdate",
    "publish": {
      "provider": "github",
      "repo": "test-auto-update",
      "owner": "1ukka",
      "private": false
    },
    "files": [
      "index.js",
      "index.html",
      "package.json",
      {
        "from": "node_modules",
        "to": "node_modules",
        "filter": ["electron-updater/**/*", "electron-log/**/*"]
      }
    ],
    "directories": {
      "buildResources": "build"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1ukka/test-auto-update"
  }
}
